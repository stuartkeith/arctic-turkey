{
	"manifest_version": 2,

	"name": "Arctic Turkey",
	"description": "Block specified sites for a specified time.",
	"version": "0.1",

	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},

	"permissions": [
		"alarms",
		"notifications",
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
	],

	"background": {
		"scripts": ["queryString.js", "background.js"]
	},

	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_popup": "popup.html"
	},

	"options_page": "options.html"
}
